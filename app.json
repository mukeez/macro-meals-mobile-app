{
  "expo": {
    "name": "macro-meals",
    "slug": "macro-meals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#009688"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.macromeals",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to find nearby meals that match your macro goals",
        "NSLocationAlwaysUsageDescription": "We need your location to find nearby meals that match your macro goals"
      }
    },
    "android": {
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#009688"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.macromeals",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlways": false,
          "locationWhenInUse": true
        }
      ]
    ]
  }
}
