{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENV": "development"
      },
      "prebuildCommand": "node scripts/copy-icons.js",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "distribution": "internal",
      "env": {
        "ENV": "staging"
      },
      "prebuildCommand": "node scripts/copy-icons.js",
      "ios": {
        "buildConfiguration": "Release",
        "autoIncrement": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "ENV": "staging"
      },
      "prebuildCommand": "node scripts/copy-icons.js",
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "env": {
        "ENV": "production",
        "ENVFILE": ".env.production"
      },
      "prebuildCommand": "node scripts/copy-icons.js",
      "ios": {
        "buildConfiguration": "Release",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "tech@emit-labs.com",
        "ascAppId": "6747797496",
        "appleTeamId": "B7JY43F6R4"
      }
    }
  }
}